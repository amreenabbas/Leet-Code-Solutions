#include <iostream>
using namespace std;

int main() {
	int n;
	int a[4][4] = {{1,4,8,10},{2,5,9,15},{6,16,18,20},{11,17,19,23}};
	cin >> n;
	for(int i = 0; i < 4; i++)
	{
		for(int j = 0; j < 4; j++)
		cout << a[i][j] << " ";
		cout << endl;
	}
	int sr = 0, sc = 0, ec = 3, er = 3;
	int r = sr, c = ec, f = 0;
	while(a[r][c] != n)
	{
		if(a[r][c] > n)
		{
			cout << a[r][c] << "->";
			c--;	
			if(c == -1)
			{
				cout << "element not present";
				f = 1;
				break;
			}
			
		}
		if(a[r][c] < n)
		{
			cout << a[r][c] << "->";
			r++;
			if(r == 4)
			{
				cout << "element not present";
				f = 1;
				break;
			}
		}
	}
	cout << endl;
	if (f == 0)
	cout << "element found at ["<< r << "," << c << "]";
	
	return 0;
}
